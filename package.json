    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "moment": "^2.29.4",
    "reload": "^3.2.1",
    "sass": "^1.69.6",
    "uninstall": "0.0.0",
    "web-push": "^3.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.22.9",
    "babel-loader": "^9.1.2",
    "chai": "^4.3.10",
