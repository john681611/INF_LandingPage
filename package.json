  "engines": {
    "node": "12.15.0"
  },
  "browserslist": [
    "> 2%",
    "last 4 versions"
  ],
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "exclude": [
      "utils/genVapidKeys.js",
