    "reporter": [
      "json-summary",
      "text",
      "html"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc_output"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "basic-auth": "^2.0.1",
    "body-parser": "^2.2.0",
