  "engines": {
    "node": "22.14.0"
  },
  "browserslist": [
    "> 2%",
    "last 4 versions"
  ],
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "exclude": [
      "utils/genVapidKeys.js",
      "**/*.test.js",
      "**/*.min.js",
      "webpack.config.js",
      "server.js",
      "test",
      "assets"
