    "mini-css-extract-plugin": "^2.9.2",
    "mocha": "^11.1.0",
    "node-mocks-http": "^1.17.0",
    "nodemon": "^3.1.10",
    "nyc": "^17.1.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^8.1.1",
    "sass-loader": "^16.0.5",
    "sinon": "20.0.0",
    "sinon-chai": "^4.0.0",
    "supertest": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1",
    "webpack-manifest-plugin": "^5.0.0"
