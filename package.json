  "engines": {
    "node": "12.15.0"
  },
  "browserslist": [
    "> 2%",
    "last 4 versions"
  ],
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "exclude": [
      "utils/genVapidKeys.js",
      "**/*.test.js",
      "**/*.min.js",
      "webpack.config.js",
      "server.js",
      "test",
      "assets"
    ],
    "reporter": [
      "json-summary",
      "text",
      "html"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc_output"
  },
  "dependencies": {
    "autoprefixer": "^10.3.6",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.19.0",
