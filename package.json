  "license": "Apache",
  "engines": {
    "node": "12.15.0"
  },
  "browserslist": [
    "> 2%",
    "last 4 versions"
  ],
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "exclude": [
      "utils/genVapidKeys.js",
      "**/*.test.js",
      "**/*.min.js",
      "webpack.config.js",
      "server.js",
      "test",
      "assets"
    ],
    "reporter": [
      "json-summary",
      "text",
      "html"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc_output"
  },
  "dependencies": {
    "autoprefixer": "^10.1.0",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "moment": "^2.29.1",
    "reload": "^3.1.1",
    "sass": "^1.32.0",
    "uninstall": "0.0.0",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
