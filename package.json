      "text",
      "html"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc_output"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.20.2",
