    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "glob": "^11.0.2",
    "moment": "^2.30.1",
    "reload": "^3.3.0",
    "sass": "^1.86.1",
    "uninstall": "0.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "babel-loader": "^10.0.0",
    "chai": "^5.2.0",
