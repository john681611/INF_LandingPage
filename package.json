    "mocha": "^10.2.0",
    "node-mocks-http": "^1.13.0",
    "nodemon": "^3.0.2",
    "nyc": "^15.1.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^7.3.4",
    "sass-loader": "^13.3.3",
    "sinon": "15.1.0",
    "sinon-chai": "^3.7.0",
