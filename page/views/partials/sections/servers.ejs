<h2>Servers</h2>
<% servers.forEach(server => { %>
  <h3>
    <%= server.name %>
  </h3>
  <p>
    <a href="<%= server.info.link %>" target="_blank" title="<%= server.info.link %>" class="postlink" rel="nofollow">
      <img alt="" resizemod="off" class="lazyload" data-src="<%= server.info.src %>" border="0" alt="" width="100%" title="">
    </a>
  </p>
  <% if(server.file){ %>
    <a class="btn" href="/modlist/<%= server.id %>" download="INF_<%= server.name %>_preset.html">Download Preset</a>
    <p class="smalltxt">Drag and drop file into Launcher</p>
    <% }%>
      <% Object.keys(server.mods).forEach(key => { %>
        <% if(server.mods[key].length > 0){ %>
          <h4>
            <%= key %>
          </h4>
          <p>
            <% server.mods[key].forEach(mod => { %>
              <a class="btn" href="https://steamcommunity.com/sharedfiles/filedetails/?id=<%= mod.steamID %>" target="_blank">
                <%= mod.name %>
              </a>
              <% }) %>
          </p>
          <% }}) %>

            <% }) %>